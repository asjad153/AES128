        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/xml_rounded_transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">

        <Button
            android:id="@+id/fled_off"
            android:layout_width="74dp"
            android:layout_height="32dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/button_grey_trans"
            android:text="off"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/fled_on"
            android:layout_width="74dp"
            android:layout_height="32dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/button_grey_trans"
            android:text="on"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/fled_off"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewf"
            android:layout_width="0dp"
